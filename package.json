{
	"name": "@volkovasystem/setup-nodejs-version",
	"alias": "setup-nodejs-version",
	"version": "0.0.19",
	"description": "Setup NodeJS Version",
	"main": "setup-nodejs-version.js",
	"scripts": {
		"check-module": "source ./setting/default-tool-setting.sh && ./tool/check-module.sh",
		"reset-module": "source ./setting/default-tool-setting.sh && ./tool/reset-module.sh",
		"clean-module": "source ./setting/default-tool-setting.sh && ./tool/clean-module.sh",
		"setup-module": "source ./setting/default-tool-setting.sh && ./tool/setup-module.sh",
		"bundle-module": "source ./setting/default-tool-setting.sh && ./tool/bundle-module.sh",
		"compress-module": "source ./setting/default-tool-setting.sh && ./tool/compress-module.sh",
		"resolve-module": "source ./setting/default-tool-setting.sh && ./tool/resolve-module.sh",
		"build-module": "source ./setting/default-tool-setting.sh && ./tool/build-module.sh",
		"test-module": "source ./setting/default-tool-setting.sh && ./tool/test-module.sh",
		"reduce-module": "source ./setting/default-tool-setting.sh && ./tool/reduce-module.sh",
		"revert-module": "source ./setting/default-tool-setting.sh && ./tool/revert-module.sh",
		"stage-module": "source ./setting/default-tool-setting.sh && ./tool/stage-module.sh",
		"release-module": "source ./setting/default-tool-setting.sh && ./tool/release-module.sh",
		"teardown-module": "source ./setting/default-tool-setting.sh && ./tool/teardown-module.sh",
		"prepublishOnly": "npm run release-module",
		"postpublish": "npm run teardown-module",
		"setup-nodejs-version": "node ./setup-nodejs-version.run.js"
	},
	"bin": {
		"setup-jq": "setup-jq.sh",
		"setup-wget": "setup-wget.sh",
		"setup-python-minimal": "setup-python-minimal.sh",
		"setup-nodejs-version": "setup-nodejs-version.sh"
	},
	"os": [
		"linux",
		"!darwin",
		"!win32"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/volkovasystem/setup-nodejs-version.git"
	},
	"keywords": [
		"setup",
		"nodejs",
		"version"
	],
	"author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
	"contributors": [
		"Richeve S. Bebedor <richeve.bebedor@gmail.com>"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/volkovasystem/setup-nodejs-version/issues"
	},
	"homepage": "https://github.com/volkovasystem/setup-nodejs-version#readme"
}
